file = {:home/bolster/Documents/Mendeley Desktop/Zouridaki et al/Ad Hoc Networks/Zouridaki et al. - 2009 - E-Hermes A robust cooperative trust establishment scheme for mobile ad hoc networks.pdf:pdf},
isbn = {1595935541},
issn = {15708705},
journal = {Ad Hoc Networks},
keywords = {Performance,Reliability,Routing,Security,Trust establishment},
number = {6},
pages = {1156--1168},
