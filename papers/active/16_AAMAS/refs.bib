journal = {2007 IEEE 65th Veh. Technol. Conf. - VTC2007-Spring},
month = {apr},
pages = {56--60},
publisher = {IEEE},
title = {{An Objective Trust Management Framework for Mobile Ad Hoc Networks}},
url = {http://ieeexplore.ieee.org/xpls/abs{\_}all.jsp?arnumber=4212452 http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=4212452},
year = {2007}
